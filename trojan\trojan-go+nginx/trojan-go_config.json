{
  "run_type": "server",
  "local_addr": "0.0.0.0",
  "local_port": 443, //监听端口
  "remote_addr": "127.0.0.1",
  "remote_port": 82, //h2回落端口
  "log_level": 1,
  "log_file": "/var/log/trojan-go/error.log", //修改成自己配置的路径，注意给够权限。
  "password": [
    "diy1155", //修改成自己配置的密码
    "diy9125" //密码可以多个，用‘，’隔开。也可删除仅保留一个。
  ],
  "disable_http_check": false,
  "udp_timeout": 60,
  "ssl": {
    "verify_hostname": true,
    "cert": "/etc/ssl/tls/xx.yy.crt", //换成你的证书，绝对路径。
    "key": "/etc/ssl/tls/xx.yy.key", //换成你的私钥，绝对路径。
    "key_password": "",
    "cipher": "",
    "curves": "",
    "prefer_server_cipher": true,
    "sni": "*.xx.yy", //修改成自己域名（通配域名）
    "alpn": [
      "h2" //仅启用h2连接。
    ],
    "session_ticket": true,
    "reuse_session": true,
    "plain_http_response": "",
    "fallback_addr": "",
    "fallback_port": 0
  },
  "tcp": {
    "no_delay": true,
    "keep_alive": true,
    "prefer_ipv4": false
  },
  "router": {
    "enabled": true, //开启路由策略，可以关闭，建议开启。
    "block": [
      "geoip:private"
    ],
    "geoip": "/usr/local/bin/trojan-go/geoip.dat", //修改成geoip.dat路径
    "geosite": "/usr/local/bin/trojan-go/geosite.dat" //修改成geosite.dat路径
  },
  "websocket": {
    "enabled": false, //默认关闭websocket支持
    "path": "/TrojanGo", //修改成自己配置的路径
    "host": "zt.xx.yy" //修改成自己域名
  },
  "shadowsocks": {
    "enabled": false,
    "method": "AES-128-GCM",
    "password": ""
  },
  "transport_plugin": {
    "enabled": false,
    "type": "plaintext",
    "command": "",
    "option": "",
    "arg": [],
    "env": []
  }
}
