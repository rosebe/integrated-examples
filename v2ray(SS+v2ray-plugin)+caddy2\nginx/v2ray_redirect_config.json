{
  "log": {
    "error": "/var/log/v2ray/error.log",
    "access": "/var/log/v2ray/access.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "listen": "127.0.0.1", //只监听本机，避免本机外的机器探测到下面端口。
      "port": "2004", //监听端口，此端口收发web代理服务器(如caddy2、nginx等）转来的数据。
      "protocol": "dokodemo-door",
      "settings": {
        "address": "v1.mux.cool",
        "network": "tcp",
        "followRedirect": false
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "path": "/SS1v2ray" //修改成自己的path
        }
      },
      "tag": "ss1ws", //标签，与routing中标签对应。
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    },
    {
      "port": 2005, //此端口必须与下边"redirect"参数中端口对应
      "protocol": "shadowsocks",
      "settings": {
        "email": "2005@gmail.com",
        "method": "chacha20-poly1305",
        "password": "diy9125", //修改成自己的密码
        "ota": false,
        "network": "tcp,udp"
      }
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "inboundTag": [
          "ss1ws" //与inbounds中标签对应
        ],
        "outboundTag": "ss1redirect" //与outbounds中标签对应
      },
      {
        "type": "field",
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "blocked"
      }
    ]
  },
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "ss1redirect", //标签，与routing中标签对应。
      "protocol": "freedom",
      "settings": {
        "redirect": "127.0.0.1:2005" //此端口必须与上边2005端口对应
      }
    },
    {
      "tag": "blocked",
      "protocol": "blackhole",
      "settings": {}
    }
  ]
}
