{
  "admin": {
    "disabled": true
  },
  "apps": {
    "http": {
      "servers": {
        "srv0": {
          "listen": [":443"],
          "routes": [{
            "match": [{
              "path": ["/SeuW56Es"] //分流出h2,与本机v2ray path对应。
            }],
            "handle": [{
              "handler": "reverse_proxy",
              "upstreams": [{
                "dial": "127.0.0.1:2002" //转发给本机v2ray对应端口
              }],
              "transport": {
                "protocol": "http",
                "keep_alive": {
                  "enabled": false
	        },
                "versions": ["h2c"]
              },
              "upstreams": [{
                "dial": "127.0.0.1:2002" //转发给本机v2ray对应端口
              }]
            }],
            "terminal": true
          },
          {
            "handle": [{
              "handler": "file_server",
              "root": "/var/www/html" //修改成自己存放的web文件路径
            }],
            "terminal": true
          }],
          "tls_connection_policies": [{
            "cipher_suites": ["TLS_ECDHE_ECDSA_WITH_AES_256_GCM_SHA384","TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305_SHA256"],
            "curves": ["x25519"],
            "alpn": ["h2","http/1.1"],
            "default_sni": "xx.yy" //修改成自己的域名
          }]
        }
      }
    },
    "tls": {
      "automation": {
        "policies": [{
          "subjects": ["xx.yy"], //修改成自己的域名
          "issuer": {
            "module": "acme"
          }
        }]
      }
    }
  }
}
