{
  "log": {},
  "inbounds": [
    {
      "port": "1080",
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    },
    {
      "port": "1081",
      "protocol": "http",
      "settings": {}
    }
  ],
  "outbounds": [
    {
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "xx.yy", //改成服务端配置的对应域名
            "port": 443, //改成服务端配置的对应端口（如没有SNI分流共用端口，就是vless+tcp 监听端口。）
            "users": [
              {
                "id": "7326a426-d601-11ea-b574-560002e9f264", //改成服务端配置的对应UUID
                "flow": "xtls-rprx-origin", //启用xtls必须增加此条参数，参数值可自己调整；否则删除此条参数。另支持xtls必须v2ray版本不小于v4.31.0，且服务端已配置支持。
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "security": "xtls", //如启用xtls，tls必须改成xtls。
        "network": "tcp",
        "tcpSettings": {}
      }
    },
    {
      "tag": "direct", //如果要使用路由，这个 tag 是一定要有的，在这里 direct 就是 freedom 的一个标号，在路由中说 direct V2Ray 就知道是这里的 freedom 了。
      "protocol": "freedom",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "IPOnDemand",
    "rules": [
      {
        "type": "field",
        "outboundTag": "direct",
        "domain": ["geosite:cn"] // 中国大陆主流网站的域名
      },
      {
        "type": "field",
        "outboundTag": "direct",
        "ip": [
          "geoip:cn", // 中国大陆的 IP
          "geoip:private" // 私有地址 IP，如路由器等
        ]
      }
    ]
  }
}
