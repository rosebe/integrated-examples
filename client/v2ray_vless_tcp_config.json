{
  "log":{},
  "inbounds":[
    {
      "port":"1080",
      "protocol":"socks",
      "settings":{
        "auth":"noauth",
        "udp":true
      }
    },
    {
      "port":"1081",
      "protocol":"http",
      "settings":{}
    }
  ],
  "outbounds":[
    {
      "protocol":"vless",
      "settings":{
        "vnext":[
          {
            "address":"xx.yy", //改成服务端配置的对应域名
            "port":443, //改成服务端配置的对应端口（如没有SNI分流共用端口，就是vless+tcp 监听端口。）
            "users":[
              {
                "id":"7326a426-d601-11ea-b574-560002e9f264", //改成服务端配置的对应UUID
                "flow": "xtls-rprx-origin", //启用xtls，必须增加此条参数；否则删除。另支持xtls必须v2ray版本不小于v4.29.0，且服务端已配置支持。
                "encryption":"none"
              }
            ]
          }
        ]
      },
      "streamSettings":{
        "security":"tls",
        "network":"tcp",
        "tcpSettings":{}
      }
    },
    {
      "tag":"direct",
      "protocol":"freedom",
      "settings":{}
    },
    {
      "tag":"blocked",
      "protocol":"blackhole",
      "settings":{}
    }
  ],
  "routing":{
    "domainStrategy":"IPOnDemand",
    "rules":[
      {
        "type":"field",
        "ip":[
          "geoip:private"
        ],
        "outboundTag":"direct"
      }
    ]
  }
}
